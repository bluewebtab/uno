    //gsap: fromTo

    window.onload = function() {
        let tween1 = gsap.from('.gsap-product-info', {duration: .50, x: -500});
        let tween3 = gsap.from('.gsap-product-image', {duration: .50, x: 500})
    }

    let slideNext = document.querySelector('.swiper-button-next');
    let slidePrev = document.querySelector('.swiper-button-prev');

    slideNext.addEventListener('click', function () {
        let tween1 = gsap.from('.gsap-product-info', {duration: .50, x: -500});
        let tween3 = gsap.from('.gsap-product-image', {duration: .50, x: 500})
    });

    slidePrev.addEventListener('click', function () {
